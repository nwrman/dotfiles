	email = jonathan.hf@globant.com
[push]
	default = current
[color]
	diff = auto
	status = auto
	branch = auto
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
[pull]
	rebase = true
[gc]
	autoDetach = false
[gitflow]
	multi-hotfix = true
